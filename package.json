{
  "name": "vsc-json2go",
  "displayName": "vsc-json2go",
  "description": "",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.51.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vsc-json2go.convertSelection",
    "onCommand:vsc-json2go.generateSelectionToClipboard"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vsc-json2go.convertSelection",
        "title": "Json2Go: Convert selected json to go type"
      },
      {
        "command": "vsc-json2go.generateSelectionToClipboard",
        "title": "Json2Go: Generate go type from selection to clipboard"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.51.0",
    "eslint": "^7.9.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0"
  },
  "dependencies": {
    "node-fetch": "^2.6.1"
  }
}
